{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "facade": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/facade",
      "sourceRoot": "apps/facade/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/facade/browser",
            "index": "apps/facade/src/index.html",
            "main": "apps/facade/src/main.ts",
            "polyfills": "apps/facade/src/polyfills.ts",
            "tsConfig": "apps/facade/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/facade/src/favicon.ico",
              "apps/facade/src/assets"
            ],
            "styles": [
              "apps/facade/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/facade/src/environments/environment.ts",
                  "with": "apps/facade/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "facade:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "facade:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "facade:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/facade/src/test.ts",
            "polyfills": "apps/facade/src/polyfills.ts",
            "tsConfig": "apps/facade/tsconfig.spec.json",
            "karmaConfig": "apps/facade/karma.conf.js",
            "assets": [
              "apps/facade/src/favicon.ico",
              "apps/facade/src/assets"
            ],
            "styles": [
              "apps/facade/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/facade/tsconfig.app.json",
              "apps/facade/tsconfig.spec.json",
              "apps/facade/e2e/tsconfig.json",
              "apps/facade/tsconfig.server.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/facade/e2e/protractor.conf.js",
            "devServerTarget": "facade:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "facade:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/facade/server",
            "main": "apps/facade/server.ts",
            "tsConfig": "apps/facade/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/facade/src/environments/environment.ts",
                  "with": "apps/facade/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "facade:build",
            "serverTarget": "facade:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "facade:build:production",
              "serverTarget": "facade:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "facade:build:production",
            "serverTarget": "facade:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "libs-screen": {
      "projectType": "library",
      "root": "libs/screen",
      "sourceRoot": "libs/screen/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "libs/screen/tsconfig.lib.json",
            "project": "libs/screen/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/screen/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/screen/src/test.ts",
            "tsConfig": "libs/screen/tsconfig.spec.json",
            "karmaConfig": "libs/screen/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/screen/tsconfig.lib.json",
              "libs/screen/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "71ab7157-3482-4d8c-ae63-ec0bf5a21e27"
  },
  "defaultProject": "facade"
}